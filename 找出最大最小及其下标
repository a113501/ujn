#include<stdio.h>
#define N 10
int main(){
	int max_index,min_index,i,a[N],min,max;
	for(i=0;i<N;i++)
		scanf("%d",&a[i]);
		
		
		
	max=a[0];
	min=a[0];
	
	
	for(i=1;i<N;i++){
		if(a[i]>max){
			max=a[i];
			max_index=i;
		}
		if(a[i]<min){
			min=a[i];
			min_index=i;
		}
	}
	
	
	
	printf("The Max Dig is %d,The Index of Max is %d\nThe Min Dig is %d,The Index of Min is %d\n",max,max_index,min,min_index);
}
